package Model;

public class CreditCard {

	private String creditCardNummber,cardType,nameOnCard,expiryDate,ccvnNum;
	
	public CreditCard(String number, String type, String name, String expiry, String ccv){
		this.creditCardNummber = number;
		this.cardType = type;
		this.nameOnCard = name;
		this.expiryDate = expiry;
		this.ccvnNum = ccv;
	}

	// Getters and Setters
	public String getCreditCardNummber() {
		return creditCardNummber;
	}

	public void setCreditCardNummber(String creditCardNummber) {
		this.creditCardNummber = creditCardNummber;
	}

	public String getCardType() {
		return cardType;
	}

	public void setCardType(String cardType) {
		this.cardType = cardType;
	}

	public String getNameOnCard() {
		return nameOnCard;
	}

	public void setNameOnCard(String nameOnCard) {
		this.nameOnCard = nameOnCard;
	}

	public String getExpiryDate() {
		return expiryDate;
	}

	public void setExpiryDate(String expiryDate) {
		this.expiryDate = expiryDate;
	}

	public String getCcvnNum() {
		return ccvnNum;
	}

	public void setCcvnNum(String ccvnNum) {
		this.ccvnNum = ccvnNum;
	}
	
}
